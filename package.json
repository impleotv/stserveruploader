{
  "name": "stserveruploader",
  "version": "1.0.3",
  "description": "StServer mission uploader",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src -d distr --presets es2015",
    "pkgWin": "pkg ./distr/app.js --targets node12-win-x64  --output stserveruploader.exe",
    "pkgLinux": "pkg ./distr/app.js --targets node12-linux-x64  --output stserveruploader.run",
    "pkgAll": "npm run pkgWin && npm run pkgLinux"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/impleotv/stserveruploader.git"
  },
  "author": "IMPLEOTV SYSTEMS LTD",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/impleotv/stserveruploader/issues"
  },
  "homepage": "https://github.com/impleotv/stserveruploader#readme",
  "dependencies": {
    "argparse": "^2.0.1",
    "axios": "^0.20.0",
    "cli-progress": "^3.8.2",
    "colors": "^1.4.0",
    "csvtojson": "^2.0.10",
    "form-data": "^3.0.0",
    "moment": "^2.29.1",
    "mqtt": "^4.1.0"
  },
  "bin": "./distr/app.js",
  "pkg": {
    "options": [],
    "assets": []
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1"
  }
